{!------------------------------Posts / Entry----------------------------!}

.entry { 
    float: left;border:1px solid {color:Body font};
    display: block;
    background-color:{color:Background};
    padding: 2px;
    margin-bottom: 5px;
    margin-left: 5px;
    {block:IndexPage}
    width: 250px; 
    {/block:IndexPage}
    {block:PermalinkPage}
    width: 400px;
    {/block:PermalinkPage}
    }

.entry img {
    {block:indexpage}max-width:250px;{/block:indexpage}
    display: block; 
    -webkit-transition: all 0.3s ease-out; 
    -moz-transition: all 0.3s ease-out; 
    transition: all 0.3s ease-out;}
    
.entry:hover {
    -webkit-transition: all 0.3s ease-out; 
    -moz-transition: all 0.3s ease-out; 
    transition: all 0.3s ease-out;
    }

.text {
    background-color:{color:Background};
    padding: 3px;
    -webkit-transition: all 0.3s ease-out; 
    -moz-transition: all 0.3s ease-out; 
    transition: all 0.3s ease-out;
    }
     

{!------------------------Titles and Headings----------------------------!}

h3 { 
    font-size: 20px; 
    line-height: 80%;
    }
   
.title { 
    font-size: 20px; 
    line-height: 80%; 
    }

.blogtitle {
    margin-top:0px; 
    margin-bottom:0px;
    }

.blogtitle a {
    font-size: 22px; 
    line-height: 80%; 
  color: {color:Body font};
  -webkit-transition: color 0.3s ease-out; 
  -moz-transition: color 0.3s ease-out; 
  transition: color 0.3s ease-out;
  }
  
.blogtitle a:hover {
   color:{color:Body font}; 
   -webkit-transition: all 0.3s ease-out; 
   -moz-transition: all 0.3s ease-out; 
   transition: all 0.3s ease-out;
   }
 
 
{!-------------------------------Links-----------------------------------!}
 
.links { text-align:center; font-size:11px; margin-top:5px;
  font-family: 'Share';  }

.links a {  letter-spacing:1px; height:25px; padding-top:1px; border:2px solid #cdcdcd; padding-bottom:1px; line-height:23px; padding-left:4px; padding-right:4px; background-color:#cdcdcd; color:#000; -webkit-transition: all 0.3s ease-out; -moz-transition: all 0.3s ease-out; transition: all 0.3s ease-out;}

.links a:hover {  background-color:#eee;border:2px solid #cdcdcd;
-webkit-transition: all 0.3s ease-out; -moz-transition: all 0.3s ease-out; transition: all 0.3s ease-out;
} 
    

{!-----------------------Indexpage Permalinks----------------------------!}

 

{--------white bars bottom--------------------}

.perma3 { 
    background-color:{color:Background};  
    padding-top:5px; 
    padding-bottom:3px; 
    opacity:0;
    margin-top:5px;
    position:absolute;padding-left:6px; padding-right:6px;
    margin-left:5px;
    text-align: center;
    -webkit-transition: all 0.3s ease-out; 
    -moz-transition: all 0.3s ease-out; 
    transition: all 0.3s ease-out;
    text-transform:lowercase;
    letter-spacing:1px;
    z-index:99;
    }
.perma3 a {
    color:{color:Body font};
    }
.entry:hover .perma3 {
    opacity: .85;
   -webkit-transition: all 0.3s ease-out; 
    -moz-transition: all 0.3s ease-out; 
    transition: all 0.3s ease-out;
    } 
    .entry:hover .perma3:hover {
    opacity: 1;
    -webkit-transition: all 0.3s ease-out; 
    -moz-transition: all 0.3s ease-out; 
    transition: all 0.3s ease-out;
    } 
    
