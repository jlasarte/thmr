<!doctype html>  

<!--[if lt IE 7 ]> <html lang="en" class="no-js ie6"> <![endif]-->
<!--[if IE 7 ]>    <html lang="en" class="no-js ie7"> <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="no-js ie8"> <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="no-js ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="author" content="Ed Rooth (edrooth.com)">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {block:Description}
    <meta name="description" content="{MetaDescription}" />
    {/block:Description}

    <!-- variable meta data -->
    <meta name="text:Flickr Username" content=""/>
    <meta name="color:Background" content="#fff" />
    <meta name="font:Title" content="Arial" />
    <meta name="font:Body" content="Arial" />
    <meta name="font:Accent" content="Lucida Sans" />
    <meta name="if:Show People I Follow" content="1" />
    <meta name="if:Show Tags" content="1" />
    <meta name="if:Show Album Art on Audio Posts" content="1" />
    <meta name="if:Enable Jump Pagination" content="0" />
    <meta name="text:Disqus Shortname" content="" />
    <meta name="image:Header" content="" />
    <meta name="image:Background" content="" />
    <link rel="stylesheet" type="text/css" href="assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="assets/css/handheld.css">
    <title>{Title}{block:SearchPage}, {lang:Search results for SearchQuery}{/block:SearchPage}{block:PostSummary}, {PostSummary}{/block:PostSummary}</title>
    
    <!-- Place favicon.ico & apple-touch-icon.png in the root of your domain and delete these references -->
    <link rel="shortcut icon" href="{Favicon}">
    <link rel="apple-touch-icon" href="{PortraitURL-128}">
    <link rel="alternate" type="application/rss+xml" href="{RSS}">
   
    <!-- variable styles --> 
    <style type="text/css">
        body {
            background: {color:Background} url('{image:Background}');
            font-family: {font:Body};
        }
    </style>
    <style type="text/css">{CustomCSS}</style>

    <script src="assets/js/libs/modernizr-1.6.min.js"></script>
</head>

<body>


    <div id="container">


        <header>
            {block:IfHeaderImage}<img src="{image:Header}" id="header-img" />{/block:IfHeaderImage}
            {block:IfNotHeaderImage}<div id="title">{Title}</div>{/block:IfNotHeaderImage}

            <div id="description">{Description}</div>

        </header>



        
        <section id="main">


            <!-- search form -->
            <section id="search">
                <form action="/search" method="get" id="search-form">
                    <input type="hidden" name="t" value="{Name}" />
                    <input type="hidden" name="scope" value="all_of_tumblr" />
                    <input type="text" name="q" class="query" value="{SearchQuery}" />
                    <input type="submit" value="{lang:Search}" class="submit" />
                    <div class="clear"></div>
                </form>
                <div id="search-scope">
                    <input type="radio" id="search-scope-me" name="scope" checked="checked" />
                    <label for="search-scope-me">{lang:My blog}</label>
                    <input type="radio" id="search-scope-all" name="scope" />
                    <label for="search-scope-all">{lang:All of Tumblr}</label>
                </div>
            </section>

 
            <!-- search results -->          
            {block:SearchPage}
                <section id="search-results">
                    {block:SearchResults}
                        {lang:SearchResultCount results for SearchQuery 2}
                    {/block:SearchResults}
                    {block:NoSearchResults}
                        {lang:No search results for SearchQuery 2}
                    {/block:NoSearchResults}
                </section>
           {/block:SearchPage}

          
            
            


            <!-- the meat and potatoes -->
            <section id="posts">
            {block:Posts}

                    
                    {block:Photo}
                        <div class="post photo">
                            {LinkOpenTag}<img src="{PhotoURL-500}" alt="{PhotoAlt}" />{LinkCloseTag}
                            {block:Caption}
                                <div class="caption">{Caption}</div>
                            {/block:Caption}
                        </div>
                    {/block:Photo}

                    
                    {block:Video}
                        <div class="post video">
                            {Video-500}
                            {block:Caption}
                                <div class="caption">{Caption}</div>
                            {/block:Caption}
                        </div>
                    {/block:Video}

                   
                    {block:Text}
                        <div class="post text">
                            {block:Title}
                                <div class="title">{Title}</div>
                            {/block:Title}
                            {block:Body}
                                <div class="body">{Body}</div>
                            {/block:Body}
                        </div>
                    {/block:Text}

                   
                    {block:Link}
                        <div class="post link">
                            <a href="{URL}" {Target}>{Name} &raquo;</a>
                            {block:Description}
                                <div class="description">{Description}</div>
                            {/block:Description}
                        </div>
                    {/block:Link}


                    {block:Quote}
                        <div class="post quote {Length}">
                            <blockquote>
                                {Quote}
                            </blockquote>
                            {block:Source}
                                <div class="source">{Source}</div>
                            {/block:Source}
                        </div>
                    {/block:Quote}
                    

                    {block:Audio}
                        <div class="post audio">
                            {block:IfShowAlbumArtOnAudioPosts}
                                {block:AlbumArt}
                                    <div class="album_art">
                                        <img src="{AlbumArtURL}" alt="{block:Artist}{Artist}{/block:Artist}{block:TrackName} - {TrackName}{/block:TrackName}" />
                                    </div>
                                {/block:AlbumArt}
                            {/block:IfShowAlbumArtOnAudioPosts}
                            
                            <div class="player">{AudioPlayerWhite}</div>
                            
                            <div class="meta">
                                {PlayCountWithLabel}
                                {block:ExternalAudio}
                                    <span class="download_external_audio"> &bull; <a href="{ExternalAudioURL}">{lang:Download}</a></span>
                                {/block:ExternalAudio}
                            </div>

                            {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
                        </div>
                    {/block:Audio}
                   
                    
                    {block:Chat}
                        <div class="post chat">
                            {block:Title}<div class="title">{Title}</div>{/block:Title}
                            <div class="lines">
                                {block:Lines}
                                    <div class="line {Alt}">{block:Label}<strong>{Label}</strong>{/block:Label} {Line}</div>
                                {/block:Lines}
                            </div>
                        </div>
                    {/block:Chat}
                       
                    
                    {block:Answer}
                        <div class="post question-answer">
                            <div class="question">{Question}</div>
                            <div class="asker"><img src="{AskerPortraitURL-24}" />{Asker}</div>
                            <div class="answer">{Answer}</div>
                        </div>
                    {/block:Answer}
           
           
                    <!-- additional post info --> 
            
                    

                   
                    
                    <div class="footer">

                        {block:Date}
                            <a href="{Permalink}">
                                 
                                 <date datetime="{Year}-{MonthNumberWithZero}-{DayOfMonthWithZero} {24HourWithZero}:{Minutes}:{Seconds}:{Beats}">{lang:Posted TimeAgo}</date>&nbsp;{lang:By PostAuthorName}

                                {block:NotReblog}
                                    <!-- original post -->
                                {/block:NotReblog}

                                {block:Reblog}
                                    {block:RebloggedFromReblog}({lang:Originally from ReblogRootName}){/block:RebloggedFromReblog}
                                {/block:Reblog}
                                
                                <div class="notes">
                                    {block:NoteCount}{NoteCountWithLabel}{/block:NoteCount} 
                                </div>
                            </a>
                        {/block:Date}

                        {block:IfShowTags}
                            {block:HasTags}
                                <div class="tags">
                                    {block:Tags}<a href="{TagURL}" class="tag">{Tag}</a>{/block:Tags}
                                </div>
                            {/block:HasTags}
                        {/block:IfShowTags}
                        {block:ContentSource}
                            <div class="content-source">
                                <a href="{SourceURL}" class="content-source-url">
                                    {lang:Source}:
                                    {block:SourceLogo}<img src="{BlackLogoURL}" width="{LogoWidth}" height="{LogoHeight}" alt="{SourceTitle}" />{/block:SourceLogo}
                                    {block:NoSourceLogo}{SourceTitle}{/block:NoSourceLogo}
                                </a>
                            </div>
                        {/block:ContentSource}
                    </div>
                  
                    
                    {block:PostNotes}
                        <div class="post-notes">{PostNotes}</div>
                    {/block:PostNotes}
                
                    
            {/block:Posts}
            </section>








            <!-- people i follow -->
            {block:IfShowPeopleIFollow}
                {block:Following}
                    <section id="following">
                        <div>{lang:Following}</div>
                        <div id="following-avatars">
                            {block:Followed}<a href="{FollowedURL}"><img src="{FollowedPortraitURL-40}" /></a>{/block:Followed}
                        </div>
                    </section>
                {/block:Following}
            {/block:IfShowPeopleIFollow}


            <!-- twitter specific stuff -->
            {block:Twitter}
            <section id="twitter">
                <a href="http://twitter.com/{TwitterUsername}" id="twitteruser-link">
                    <div>{lang:Latest Tweets}</div>
                </a>
                <div id="tweets"></div>
            </section>
            {/block:Twitter}
            




            <!-- nav elements -->

            {block:HasPages}
                <nav id="page-links">
                    {block:Pages}<a href="{URL}">{Label}</a>{/block:Pages}
                </nav>
            {/block:HasPages}

            <nav id="meta-links">
                <a href="{RSS}">{lang:RSS feed}</a>
                <a href="/random">{lang:Random}</a>
                <a href="/archive">{lang:Archive}</a>
                <a href="/mobile">{lang:Mobile}</a>
                <a href="http://www.tumblr.com/follow/{Name}" id="followontumblr-link">{lang:Follow on Tumblr}</a>
                <!-- the "ask" link -->
                {block:AskEnabled}<a href="/ask" id="ask-link">{AskLabel}</a>{/block:AskEnabled}
                <!-- the "submit" link -->
                {block:SubmissionsEnabled}<a href="/submit" id="submit-link">{SubmitLabel}</a>{/block:SubmissionsEnabled}
            </nav>



            {block:Pagination}
                <nav id="navigation" {block:IfEnableJumpPagination}class="jump_pagination"{/block:IfEnableJumpPagination}>
                    {block:PreviousPage}<a href="{PreviousPage}">&larr; {lang:Previous}</a>{/block:PreviousPage}
                    
                    {block:IfEnableJumpPagination}
                        {block:JumpPagination length="5"}
                            {block:CurrentPage}
                                <span class="current_page">{PageNumber}</span>
                            {/block:CurrentPage}
                            
                            {block:JumpPage}
                                <a class="jump_page" href="{URL}">{PageNumber}</a>
                            {/block:JumpPage}
                            
                        {/block:JumpPagination}
                    {/block:IfEnableJumpPagination}
                    
                    {block:NextPage}<a href="{NextPage}">{lang:Next page} &rarr;</a>{/block:NextPage}
                </nav>
            {/block:Pagination}






            <div class="clearfix"></div>
        </section>
        <!-- end of main section -->
        
        <footer>
            &copy; {CopyrightYears} <a href="http://www.tumblr.com">{lang:Powered by Tumblr}</a>
        </footer>
    </div>
    <!-- end of #container -->



















    <!-- load scripts last -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js"></script>
    <script>!window.jQuery && document.write(unescape('%3Cscript src="js/libs/jquery-1.5.0.js"%3E%3C/script%3E'))</script>
    <script src="js/plugins.js"></script>
    <script src="js/script.js"></script>
    
    
    {block:Twitter}
        <script src="/tweets.js"></script>
    {/block:Twitter}
    
    {block:IfDisqusShortname}
        <script>
            //<![CDATA[
            (function() {
                var links = document.getElementsByTagName('a');
                var query = '?';
                for(var i = 0; i < links.length; i++) {
                    if(links[i].href.indexOf('#disqus_thread') >= 0) {
                        query += 'url' + i + '=' + encodeURIComponent(links[i].href) + '&';
                    }
                }
                document.write('<script charset="utf-8" src="http://disqus.com/forums/{text:Disqus Shortname}/get_num_replies.js' + query + '"></' + 'script>');
            })();
            //]]>
        </script>
    {/block:IfDisqusShortname} 
    
      
    <!--[if lt IE 7 ]>
    <script src="js/libs/dd_belatedpng.js"></script>
    <script> DD_belatedPNG.fix('img, .png_bg'); </script>
    <![endif]-->

